<h2>Benchmark Report using #<%= @queried_through %></h2>
<h3>Fetched Users: <%= @users.count %></h3>

<table>
  <thead>
    <tr>
      <th></th>
      <th>User CPU Time</th>
      <th>System CPU Time</th>
      <th>Sum</th>
      <th>Elapsed Real Time</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th>#<%= @queried_through %></th>
      <td><%= @benchmark_report.utime %></td>
      <td><%= @benchmark_report.stime %></td>
      <td><%= @benchmark_report.total %></td>
      <td><%= @benchmark_report.real %></td>
    </tr>
  </tbody>
</table>